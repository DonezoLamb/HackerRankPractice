#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int firstNum, secNum, thirdNum;
     
    cin >>firstNum >> secNum >> thirdNum;
    
    cout<< firstNum + secNum + thirdNum << endl;
    
    return 0;
}
